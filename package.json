{
  "name": "vscode-readme-editor",
  "displayName": "VS Code ReadMe Editor",
  "description": "Create, Customize and Commit your project's ReadMe in VS Code with an easy to use visual interface",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "vscode-extension",
    "Readme",
    "ReadMe",
    "ReadMe generator",
    "ReadMe editor",
    "ReadMe maker"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-readme-editor.open",
        "title": "VS Code ReadMe Editor"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SumitNalavade/VSCode-ReadMe-Editor.git"
  },
  "scripts": {
    "vscode:prepublish": "",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1",
    "zustand": "^4.0.0"
  }
}
